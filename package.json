{
  "name": "tsw-hud-project",
  "version": "1.0.0",
  "description": "TSW HUD Project - Compilable dashboard with SQLite database",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "node build.js",
    "build:runtime": "node build_runtime.js"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "assets": [
      "public/**/*",
      "node_modules/sql.js/dist/sql-wasm.wasm"
    ]
  },
  "keywords": [
    "tsw5",
    "tsw6",
    "dashboard",
    "hud",
    "train-simulator"
  ],
  "author": "HARRY FAIRBANKS",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "csv-writer": "^1.6.0",
    "sharp": "^0.33.0",
    "sql.js": "^1.10.0",
    "tesseract.js": "^5.0.4"
  }
}
